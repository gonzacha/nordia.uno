  "keywords": [
    "elecciones",
    "corrientes",
    "resultados",
    "electorales",
    "dashboard",
    "vanilla-js",
    "es6-modules"
  ],
  "author": "GT Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gtteam/gt-intelligence.git"
  },
  "bugs": {
    "url": "https://github.com/gtteam/gt-intelligence/issues"
  },
  "homepage": "https://github.com/gtteam/gt-intelligence#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "Chrome >= 61",
    "Firefox >= 60",
    "Safari >= 11",
    "Edge >= 16"
  ],
  "devDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {},
